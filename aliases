alias g='git'
alias gs='git status '
alias gst='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
alias go='git checkout '

alias flushcache='dscacheutil -flushcache'
alias top='top -ocpu'
alias unload='launchctl unload -w'
alias load='launchctl load -w'
alias start_mysql='load ~/Library/LaunchDaemons/com.mysql.mysqld.plist'
alias stop_mysql='unload ~/Library/LaunchDaemons/com.mysql.mysqld.plist'
alias ev="vi ~/.vimrc"
alias eb="vi ~/.bashrc"
alias sb="source ~/.bashrc"
alias ez="vi ~/.zshrc"
alias sz="source ~/.zshrc"

alias ll="ls -lah"
alias lm='ll | less'
alias sl=ls

[ $has_mvim ] && alias vi=$NB_EDITOR
